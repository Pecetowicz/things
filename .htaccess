#ALLOW ONLY FOR LISTED COUNTRIES
<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{ENV:GEOIP_COUNTRY_CODE} !^(MO|DE|US|UK|IE|NO|SI|CA|FR)$
RewriteRule ^(.*)$ - [F]
</ifModule>

